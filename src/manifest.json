{
    "name": "Totamjung",
    "description": "BOJ Algorithm Tag Locker",
    "version": "1.0",
    "manifest_version": 3,
    "action": {},
    "permissions": [
        "storage",
        "activeTab",
        "scripting",
        "tabs"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
            "matches": [
                "https://www.acmicpc.net/*"
            ],
            "run_at": "document_start",
            "js": [
                "theme_loader.js"
            ],
            "css": [
                "pallette.css",
                "theme.css"
            ]
        },
        {
            "matches": [
                "https://www.acmicpc.net/problem/*"
            ],
            "run_at": "document_idle",
            "js": [
                "tag_locker.js"
            ],
            "css": [
                "tag_locker.css"
            ]
        }
    ],
    "options_page": "options.html"
}